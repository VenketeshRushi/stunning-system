{
  "name": "backend",
  "version": "1.0.0",
  "description": "Production-ready Node.js Express backend with TypeScript",
  "private": true,
  "type": "module",
  "main": "dist/server.js",
  "packageManager": "pnpm@10.18.2",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx watch server.ts",
    "build": "tsc --project tsconfig.json",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "start:inspect": "cross-env NODE_ENV=production node --inspect dist/server.js",
    "start:inspect:debug": "cross-env NODE_ENV=production node --inspect-brk dist/server.js",
    "db:generate": "tsx drizzle-kit generate",
    "db:migrate": "cross-env NODE_ENV=development tsx scripts/database/migrate.ts",
    "db:seed": "cross-env NODE_ENV=development tsx scripts/database/seed.ts",
    "db:reset": "cross-env NODE_ENV=development tsx scripts/database/reset.ts",
    "db:fresh": "pnpm run db:reset && pnpm run db:migrate && pnpm run db:seed",
    "db:push": "tsx drizzle-kit push",
    "db:drop": "tsx drizzle-kit drop",
    "db:studio": "tsx drizzle-kit studio",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "audit": "pnpm audit --audit-level=high",
    "audit:fix": "pnpm audit fix",
    "deps:update": "pnpm update --interactive --latest",
    "deps:check": "pnpm outdated",
    "validate": "pnpm run type-check && pnpm run format:check"
  },
  "dependencies": {
    "@langchain/core": "^1.0.2",
    "@langchain/google-genai": "^1.0.3",
    "@langchain/langgraph": "^1.0.2",
    "@langchain/openai": "^1.0.0",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.5",
    "express": "^5.1.0",
    "google-auth-library": "^10.5.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^1.0.2",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "pg": "^8.16.3",
    "qs": "^6.14.1",
    "twilio": "^5.10.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/hpp": "^0.2.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.6.2",
    "@types/nodemailer": "^7.0.1",
    "@types/pg": "^8.15.5",
    "@types/qs": "^6.14.0",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.4",
    "prettier": "^3.6.2",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
